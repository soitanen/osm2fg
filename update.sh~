#!/bin/bash

while getopts "t:" OPTION
do
     case $OPTION in
     	t)
             tg=$OPTARG
             ;;
     esac
done

cd $tg
cd ..
cd ..
cd ..
sh download_and_compile.sh OSG
sh download_and_compile.sh SIMGEAR
sh terragear_download_and_compile.sh TERRAGEAR

git clone https://www.github.com/soitanen/osm2fg.git
